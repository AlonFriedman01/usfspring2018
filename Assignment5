A<-matrix(1:100, nrow=6)
Aseq<-matrix(seq(1,100,100/36), nrow=6)

solve(A)
det(A)
solve(Aseq)
det(Aseq)

B<-matrix(1:1000, nrow=6)
Bseq<-matrix(seq(1,1000,1000/36), nrow=6)

solve(B)
det(B)
solve(Bseq)
det(Bseq)

C<-matrix(c(4,6,8,9,4,2,1,12,5,5,7,12,735,886,23,753,78,4,5,12,54,68,23,99,64,26,4,1,8,3,57,23,88,12,54,8), nrow=6)

solve(C)
det(C)
